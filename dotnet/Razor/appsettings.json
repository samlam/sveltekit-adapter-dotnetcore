{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "NodejsOptions": {
    "Concurrency": "MultiProcess",
    "ConcurrencyDegree": 2,
    "NodejsConnectionTimeoutMS": 500,
    "NodeAndV8Options": "--es-module-specifier-resolution=node --experimental-vm-modules",
    "ScriptPath": "./build/index.cjs",
    "GzipCompressResponse": false,
    "BodyOnlyReply": true,
    "DotNetPort": 5004,
    "DotNetScheme": "http",
    "NodeEnv": "production"
  }
}
