{
  "name": "sveltekit-dotnet",
  "version": "0.1.0",
  "description": "sveltekit-dotnet sample project",
  "scripts": {
    "dev": "cross-env VITE_ForgePort=5004 svelte-kit dev",
    "build": "svelte-kit build --verbose",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:pretty": "prettier --check --plugin-search-dir=. . ",
    "test": "echo \"Error: no test specified\" && exit 0",
    "clean": "rimraf ./.svelte-kit && rimraf ./build && rimraf ./node_modules"
  },
  "keywords": [
    "forge",
    "svelte",
    "sveltekit",
    "c#"
  ],
  "type": "module",
  "engines": {
    "node": ">=16.4.0",
    "npm": ">=7.9.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.0.0-next.326",
    "@tsconfig/svelte": "^2.0.1",
    "@types/cookie": "^0.4.0",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.13.14",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "node-fetch": "^2.6.6",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.43.4",
    "svelte": "^3.47.0",
    "svelte-check": "^2.7.0",
    "svelte-icons": "^2.1.0",
    "svelte-preprocess": "^4.10.6",
    "svelte-toggle": "^3.1.0",
    "sveltekit-adapter-dotnetcore": "workspace:*",
    "tslib": "^2.3.1",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "svelte-i18n": "^3.4.0"
  }
}
