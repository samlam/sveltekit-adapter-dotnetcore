{
  "name": "sveltekit-dotnet",
  "version": "0.1.0",
  "description": "sveltekit-dotnet sample project",
  "scripts": {
    "dev": "cross-env VITE_DotNetPort=5004 NODE_OPTIONS=\"--inspect=9222\" svelte-kit dev",
    "build": "svelte-kit build --verbose",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:pretty": "prettier --check --plugin-search-dir=. . ",
    "test": "echo \"Error: no test specified\" && exit 0",
    "clean": "rimraf ./.svelte-kit && rimraf ./build && rimraf ./node_modules"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "c#"
  ],
  "type": "module",
  "engines": {
    "node": ">=16.4.0",
    "npm": ">=7.9.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.0.0-next.335",
    "@tsconfig/svelte": "^3.0.0",
    "@types/cookie": "^0.5.1",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.39",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "node-fetch": "^3.2.4",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.51.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-icons": "^2.1.0",
    "svelte-preprocess": "^4.10.6",
    "svelte-toggle": "^3.1.0",
    "sveltekit-adapter-dotnetcore": "workspace:*",
    "tslib": "^2.4.0",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "svelte-i18n": "^3.4.0"
  }
}
